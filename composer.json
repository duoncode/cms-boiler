{
	"name": "duon/cms-boiler",
	"description": "Boiler templates for Duon CMS",
	"type": "library",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "ebene fÃ¼nf GmbH",
			"email": "duon@ebenefuenf.de",
			"role": "Developer"
		}
	],
	"homepage": "https://duon.dev/cms-boiler",
	"keywords": [
		"duon",
		"cms",
		"templating",
		"templates",
		"boiler"
	],
	"autoload": {
		"psr-4": {
			"Duon\\Cms\\Boiler\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Duon\\Cms\\Boiler\\Tests\\": "tests"
		}
	},
	"require": {
		"php": ">=8.1",
		"duon/boiler": "dev-main"
	},
	"require-dev": {
		"duon/cms": "dev-main",
		"duon/dev": "^1.1",
		"duon/log": "dev-main",
		"ernst/coverlyzer": "^0.2",
		"dealerdirect/phpcodesniffer-composer-installer": "^1.0",
		"nyholm/psr7": "^1.5",
		"nyholm/psr7-server": "^1",
		"phpunit/phpunit": "^12.0",
		"staabm/annotate-pull-request-from-checkstyle": "^1.8",
		"vimeo/psalm": "^6.10",
		"filp/whoops": "^2.15"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
